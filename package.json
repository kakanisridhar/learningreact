{
  "name": "learning_react",
  "version": "1.0.0",
  "description": "Learning react",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev launch jsonserver",
    "launch": "openfin --launch --config src/assets/app.json",
    "dev": "cross-env NODE_ENV=local node webpack/webpack-dev-server.js --inspect",
    "jsonserver": "json-server --watch db.json --port 3001",
    "build:dev": "rimraf build && cross-env NODE_ENV=production webpack --config webpack/common.webpack.js",
    "start:prod": "cross-env serve -s ./build/",
    "build:prod": "rimraf build && cross-env NODE_ENV=production webpack --config webpack/prod.webpack --progress",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint-fix": "eslint **/*/*.tsx  --fix"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "dependencies": {
